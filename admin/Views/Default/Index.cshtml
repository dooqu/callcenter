@{Layout = "~/Views/Shared/_Layout.cshtml";}
<div class="div_oms_main">
    <div class="div_oms_main_left">
        <div class="div_content_logo"></div>
        <div class="div_oms_content_logo"></div>
        <!-- 菜单 -->
        <div class="div_main_left_menu_01_a" name="menu" onclick="ShowContent('JobIndex/0', 'false');">贷款结清证明申请</div>
        <div class="div_main_left_menu_02" name="menu" onclick="ShowContent('JobIndex/1', 'false');">手机号变更申请</div>
        <div class="div_main_left_menu_03" name="menu" onclick="ShowContent('JobIndex/2', 'false');">银行卡变更申请</div>
        <div class="div_main_left_menu_04" name="menu" onclick="ShowContent('JobIndex/3', 'false');">征信查询核实申请</div>
        <div class="div_oms_main_exit" onclick="javascript: window.location.href = '/Platform/OMSDefault'"></div>
    </div>
    <div class="div_oms_main_right">
        <div class="div_oms_user_site">
            <div class="div_oms_user_info"></div>
            <div class="div_oms_user_info_login">
                @Session["UserName"]
            </div>
        </div>

        <div class="div_oms_content_site">
            <div class="div_oms_content_site_02" id="Navigator"></div>
            <div class="div_model_exit"></div>
            <div class="div_site_com" id="EntityInfo"></div>
        </div>
        <div class="div_oms_menu_barNew_site">
           
        </div>
        <iframe src="" frameborder="0" framespacing="0" class="frm_Content" width="100%" height="100%"></iframe>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $(".frm_Content").attr("src", "JobIndex/0");
    });
</script>